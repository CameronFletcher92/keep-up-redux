{
  "name": "keep-up-redux",
  "version": "0.1.0",
  "description": "Personal training client tracking application",
  "scripts": {
    "dev": "NODE_ENV=development nodemon --watch server --harmony server/server.js",
    "clean": "rm -rf client/build www/ platforms plugins",
    "build": "NODE_ENV=production webpack -p --progress --colors --config config/webpack.prod.config.js",
    "start": "NODE_ENV=production node --harmony server/server.js",
    "postinstall": "npm run build",
    "deploy": "npm run clean && git push heroku master",
    "cordova": "npm run clean && NODE_ENV=production webpack -p --progress --colors --config config/webpack.prod.ios.config.js && cordova platforms add ios && cordova plugin add cordova-plugin-crosswalk-webview && cordova run ios"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cameronfletcher92/keep-up-redux.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "redux",
    "boilerplate",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack",
    "babel",
    "react-transform"
  ],
  "author": "Cameron Fletcher (http://github.com/cameronfletcher92)",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^4.1.3",
    "babel-plugin-react-transform": "^1.1.1",
    "eslint": "^1.7.3",
    "eslint-config-airbnb": "^0.1.0",
    "eslint-plugin-react": "^3.6.3",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "script-loader": "^0.6.1",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.4.1"
  },
  "dependencies": {
    "autoprefix": "^0.1.4",
    "babel-core": "^5.8.29",
    "babel-loader": "^5.3.2",
    "babel-plugin-react-autoprefix": "^0.2.6",
    "body-parser": "^1.14.1",
    "express": "^4.13.3",
    "express-session": "^1.12.0",
    "faker": "^3.0.1",
    "history": "^1.12.6",
    "html-webpack-plugin": "^1.6.2",
    "immutable": "^3.7.5",
    "lodash": "^3.10.1",
    "material-ui": "^0.13.0",
    "mongoose": "^4.2.3",
    "passport": "^0.3.0",
    "passport-google-oauth": "^0.2.0",
    "react": "0.14.0",
    "react-dom": "^0.14.0",
    "react-immutable-proptypes": "^1.2.3",
    "react-pure-render": "^1.0.2",
    "react-redux": "^4.0.0",
    "react-router": "^1.0.0-rc3",
    "react-tap-event-plugin": "^0.2.1",
    "redux": "^3.0.4",
    "redux-devtools": "^2.1.5",
    "redux-router": "^1.0.0-beta3",
    "redux-thunk": "^1.0.0",
    "superagent": "^1.4.0",
    "webpack": "^1.12.2"
  }
}
